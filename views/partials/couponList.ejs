<div class="couponList" id="couponList">
      <ul class="list-group">
        <li class="list-group-item list-group-item-info" id="couponList-title">
          <h4> CouponCode </h4>
          <h4> Username </h4>
          <h4> Valid </h4>
          <h4> couponType </h4>
          <h4> salesCode </h4>
          <h4> Coupon Rule </h4>
          <h4> Rebate Rule </h4>

        </li>

        <!-- List All Coupon Code -->
        <% CouponList.forEach(function(coupon) { %>
          <li class="list-group-item">
            <h4 id="couponItem"><%= coupon.couponID %></h4>
            <h4><%= coupon.username %></h4>
            <h4><%= coupon.valid %></h4>
            <h4><%= coupon.couponType %></h4>
            <h4><%= coupon.salesCode %></h4>
            <h4><%= coupon.couponRule.type %> <%= coupon.couponRule.value %></h4>
            <h4><%= coupon.rebateRule.type %> <%= coupon.rebateRule.value %></h4>
            <a href="/views/coupons/<%= coupon.couponID %>">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span></a>
          </li>
        <% }); %>

      </ul>
</div>